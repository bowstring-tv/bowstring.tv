{
  "name": "bowstring.tv",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/www",
    "start:dev": "nodemon ./src/bin/www",
    "postcss": "postcss ./src/styles/main.css -o ./public/stylesheets/main.css",
    "postcss:dev": "postcss ./src/styles/main.css -o ./public/stylesheets/main.css -w",
    "dev": "concurrently \"npm:start:dev\" \"npm:postcss:dev\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "^4.1.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2"
  }
}